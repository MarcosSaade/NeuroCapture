{
  "name": "neurocapture-frontend",
  "version": "0.1.0",
  "description": "Electron desktop application for NeuroCapture neurological assessment platform",
  "main": "main/index.js",
  "private": true,
  "homepage": "./",
  "author": "NeuroCapture Development Team",
  "license": "PROPRIETARY",
  "scripts": {
    "dev:renderer": "vite --config renderer/vite.config.js",
    "build:renderer": "vite build --config renderer/vite.config.js",
    "dev:electron": "cross-env NODE_ENV=development electron ./main",
    "start:electron": "cross-env NODE_ENV=production electron ./main",
    "dev": "concurrently \"npm run dev:renderer\" \"npm run dev:electron\"",
    "build": "npm run build:renderer",
    "test": "echo \"Frontend tests not yet implemented\" && exit 0",
    "lint": "eslint renderer/src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint renderer/src --ext .js,.jsx,.ts,.tsx --fix"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.2",
    "@heroicons/react": "^2.2.0",
    "axios": "^1.9.0",
    "electron": "^29.4.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.5.3"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.5",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "tailwindcss": "^3.4.0",
    "vite": "^5.4.19"
  },
  "electronVersion": "29.4.6",
  "keywords": [
    "electron",
    "react",
    "neuroscience",
    "cognitive-assessment",
    "desktop-app"
  ]
}
